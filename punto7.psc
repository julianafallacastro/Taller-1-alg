Proceso punto7 //Calcular la media de tres números pedidos por teclado.
	Definir a, b, c como entero;
	Escribir "Ingresa 3 números";
	//leer a, b , c;
	a =30;
	b =20;
	c=40;
	si a <>b y a <> c y b<>c entonces;
		si a>b y a < c entonces;
			Escribir "El número medio es:", a;
		FinSi
	SiNo
